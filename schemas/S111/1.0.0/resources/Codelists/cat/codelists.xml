      <gco:Date>2018-11-05</gco:Date>
   </cat:versionDate>
   

<cat:codelistItem>
      <cat:CT_Codelist id="S111_MD_TypeOfCurrentData">
         <cat:identifier>
            <gco:ScopedName codeSpace="http://www.iho.int/S111/1.0.0/md/1.0">S111_MD_TypeOfCurrentData</gco:ScopedName>
         </cat:identifier>
         <cat:name>
            <gco:ScopedName codeSpace="http://www.iho.int/S111/1.0.0/md/1.0">S111_MD_TypeOfCurrentData</gco:ScopedName>
         </cat:name>
         <cat:definition>
            <gco:CharacterString>Types of surface current data, based on the source of the data</gco:CharacterString>
         </cat:definition>
         <cat:description>
            <gco:CharacterString>Types of surface current data, based on the source of the data</gco:CharacterString>
         </cat:description>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="S111_MD_TypeOfCurrentData_historicalObservation">
               <cat:identifier>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">historicalObservation</gco:ScopedName>
               </cat:identifier>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">historicalObservation</gco:ScopedName>
               </cat:name>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData/numCode">1</gco:ScopedName>
               </cat:name>
               <cat:definition>
                  <gco:CharacterString>Historical observation</gco:CharacterString>
               </cat:definition>
               <description><gco:CharacterString>Observation made hours, days, etc., in the past</gco:CharacterString></description>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="S111_MD_TypeOfCurrentData_realTimeObservation">
               <cat:identifier>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">realTimeObservation</gco:ScopedName>
               </cat:identifier>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">realTimeObservation</gco:ScopedName>
               </cat:name>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData/numCode">2</gco:ScopedName>
               </cat:name>
               <cat:definition>
                  <gco:CharacterString>Real-time observation</gco:CharacterString>
               </cat:definition>
               <description><gco:CharacterString>Observation no more than a few minutes old</gco:CharacterString></description>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="S111_MD_TypeOfCurrentData_astronomicalPrediction">
               <cat:identifier>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">astronomicalPrediction</gco:ScopedName>
               </cat:identifier>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">astronomicalPrediction</gco:ScopedName>
               </cat:name>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData/numCode">3</gco:ScopedName>
               </cat:name>
               <cat:definition>
                  <gco:CharacterString>Astronomical prediction</gco:CharacterString>
               </cat:definition>
               <description><gco:CharacterString>Value computed using harmonic constants only</gco:CharacterString></description>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="S111_MD_TypeOfCurrentData_analysisOrHybridMethod">
               <cat:identifier>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">analysisOrHybridMethod</gco:ScopedName>
               </cat:identifier>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">analysisOrHybridMethod</gco:ScopedName>
               </cat:name>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData/numCode">4</gco:ScopedName>
               </cat:name>
               <cat:definition>
                  <gco:CharacterString>Analysis or hybrid method</gco:CharacterString>
               </cat:definition>
               <description><gco:CharacterString>Calculation by statistical or other indirect methods, or a combination of methods</gco:CharacterString></description>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="S111_MD_TypeOfCurrentData_hydrodynamicModelHindcast">
               <cat:identifier>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">hydrodynamicModelHindcast</gco:ScopedName>
               </cat:identifier>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">hydrodynamicModelHindcast</gco:ScopedName>
               </cat:name>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData/numCode">5</gco:ScopedName>
               </cat:name>
               <cat:definition>
                  <gco:CharacterString>Hydrodynamic model hindcast</gco:CharacterString>
               </cat:definition>
               <description><gco:CharacterString>Gridded data from a two- or three-dimensional dynamic simulation of past conditions using only observed data for boundary forcing</gco:CharacterString></description>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
         <cat:codeEntry>
            <cat:CT_CodelistValue id="S111_MD_TypeOfCurrentData_hydrodynamicModelForecast">
               <cat:identifier>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">hydrodynamicModelForecast</gco:ScopedName>
               </cat:identifier>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData">hydrodynamicModelForecast</gco:ScopedName>
               </cat:name>
               <cat:name>
                  <gco:ScopedName codeSpace="#S111_MD_TypeOfCurrentData/numCode">6</gco:ScopedName>
               </cat:name>
               <cat:definition>
                  <gco:CharacterString>Hydrodynamic model forecast</gco:CharacterString>
               </cat:definition>
               <description><gco:CharacterString>Gridded data from a two- or three-dimensional dynamic simulation of future conditions using predicted data for boundary forcing</gco:CharacterString></description>
            </cat:CT_CodelistValue>
         </cat:codeEntry>
      </cat:CT_Codelist>
   </cat:codelistItem>
</CT_CodelistCatalogue>
